blueprint Tester
  starts value at 10
  starts items as empty

when test_calc
  calc 5 plus 3 as sum
  set value to sum
finfr "Calc works"

when test_array
  change items by + "apple"
  change items by + "banana"
  set Screen.text to "Array has items"
finfr "Array works"

when test_must
  must value is above 5
    otherwise "Value too low"
  set Screen.text to "Must passed"
finfr "Must works"

when test_block
  block if value is below 100
  set Screen.text to "Block failed"
finfr "Block works"
